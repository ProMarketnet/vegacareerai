{
  "name": "vega-career-ai",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "vercel-build": "vite build",
    "start": "vite preview",
    "server": "node api/production-server.js",
    "server:dev": "nodemon api/production-server.js",
    "server:simple": "node api/simple-server.js",
    "dev:full": "concurrently \"npm run dev\" \"npm run server:dev\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "lint": "eslint src api --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src api --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\" \"api/**/*.js\"",
    "audit:security": "npm audit --audit-level moderate",
    "db:migrate": "npx prisma migrate dev",
    "db:generate": "npx prisma generate",
    "db:studio": "npx prisma studio",
    "logs:error": "tail -f logs/error.log",
    "logs:combined": "tail -f logs/combined.log"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.37.0",
    "@google/generative-ai": "^0.24.1",
    "@prisma/client": "^6.9.0",
    "@stripe/stripe-js": "^7.3.1",
    "@supabase/auth-helpers-react": "^0.5.0",
    "@supabase/supabase-js": "^2.50.0",
    "@vercel/node": "^5.2.1",
    "axios": "^1.10.0",
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.1",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.453.0",
    "openai": "^5.3.0",
    "pg": "^8.16.0",
    "rate-limiter-flexible": "^7.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.6.2",
    "stripe": "^18.2.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "babel-jest": "^30.0.0",
    "concurrently": "^9.1.2",
    "eslint": "^9.29.0",
    "husky": "^9.1.7",
    "jest": "^30.0.0",
    "lint-staged": "^16.1.2",
    "nodemon": "^3.1.10",
    "postcss": "^8.4.38",
    "prettier": "^3.5.3",
    "supertest": "^7.1.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  }
}
